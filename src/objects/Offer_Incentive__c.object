        <formula>IF(ISNULL( Valid_Start_Date__c), 0, 
IF(ISNULL(  Valid_End_Date__c), 0,
IF ( (Valid_End_Date__c - Valid_Start_Date__c) &lt;0, 0,

(Valid_End_Date__c - Valid_Start_Date__c) * 24 * 60 + 
(24 - ( value(text(Valid_Start_Hour__c)) +  value(text(Valid_Start_Minute__c)) )) * 60   +  value(text(Valid_End_Hour__c))  +  value(text(Valid_End_Minute__c)) 



