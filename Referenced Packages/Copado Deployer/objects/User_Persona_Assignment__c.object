    <validationRules>
        <fullName>Permanent_no_Dates</fullName>
        <active>true</active>
        <errorConditionFormula>and (
 ISPICKVAL( Type__c ,&quot;Permanent&quot;),
  or (
    not( ISNULL( End_Time__c ) ),
    not( ISNULL( Start_Time__c  ) )
  )
)</errorConditionFormula>
        <errorDisplayField>Type__c</errorDisplayField>
        <errorMessage>Cannot  specify End and Start time for Permanent permissions.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Temporary_with_Dates</fullName>
        <active>true</active>
        <errorConditionFormula>and (
 ISPICKVAL( Type__c ,&quot;Temporary&quot;),
  or (
   ISNULL( End_Time__c ),
   ISNULL( Start_Time__c  )
  )
)</errorConditionFormula>
        <errorDisplayField>Type__c</errorDisplayField>
        <errorMessage>Specify End and Start time for Temporary permissions.</errorMessage>
    </validationRules>
